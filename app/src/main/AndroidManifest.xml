<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.speedwagon.certificatekeeper">
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-feature android:name="android.hardware.camera"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Certificatekeeper">
        <activity
            android:name=".CertificateView"
            android:exported="false" />
        <activity
            android:name=".Demo"
            android:exported="false" />
        <activity
            android:name=".Home"
            android:exported="false"
            android:label="Home"
            android:theme="@style/Theme.Certificatekeeper.NoActionBar"/>
        <activity
            android:name=".FileExplorer"
            android:exported="false" />
        <activity
            android:name=".Profile"
            android:exported="false"
            android:theme="@style/Theme.Certificatekeeper.NoActionBar"
            android:parentActivityName=".Home"/>
        <activity
            android:name=".LoginRegister"
            android:exported="false"
            android:theme="@style/Theme.Certificatekeeper.NoActionBar"/>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.Certificatekeeper.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>